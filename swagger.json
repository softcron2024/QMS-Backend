                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "ResponseCode": {
                                            "type": "string"
                                        },
                                        "message": {
                                            "type": "array",
                                            "items": {
